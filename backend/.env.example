# Jobber OAuth Configuration
# Replace with your actual credentials from Jobber Developer Console

# Required: Jobber OAuth App Credentials
JOBBER_CLIENT_ID=your_client_id_here
JOBBER_CLIENT_SECRET=your_client_secret_here

# Required: OAuth Redirect URL (must match your Jobber app config)
JOBBER_REDIRECT_URL=http://localhost:3000/auth/callback

# Required: Jobber API Base URL
JOBBER_API_URL=https://api.getjobber.com/api/graphql

# Required: Client app origin for CORS
CLIENT_APP_ORIGIN=http://localhost:3000

# Required: Invoice date filtering field
INVOICE_DATE_FIELD=createdAt

# Required: Data source configuration
USE_REAL_DATA=false

# Required: Session and token management
SESSION_SECRET=your_session_secret_here
TOKEN_STORAGE_PATH=./tokens.json

# Required: Debug flags
DEBUG_OAUTH=false
DEBUG_GRAPHQL=false

# Required: Database configuration (for production)
DATABASE_HOST=localhost
DATABASE_PORT=5432
DATABASE_USERNAME=postgres
DATABASE_PASSWORD=your_database_password_here

# Required: Redis configuration (for production caching)
REDIS_URL=redis://localhost:6379/1
SIDEKIQ_REDIS_URL=redis://localhost:6379/1

# Required: Jobber API version
JOBBER_GRAPHQL_VERSION=2025-01-20

# Required: Admin operations
ADMIN_KEY=your_admin_key_here
ADMIN_REFRESH_MIN_INTERVAL=15

# Required: Business Rates (adjust for your business)
SUPPORT_PLUMBER_RATE=16.70
FIXED_OVERHEAD_RATE=18.07
LEAD_PLUMBER_RATE_WES=34.85
LEAD_PLUMBER_RATE_LORIN=24.76
LEAD_PLUMBER_RATE_ELIJAH=23.78